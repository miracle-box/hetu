{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "gen:component": "turbo gen react-component",
    "typecheck": "tsc --noEmit",
    "typecheck:tsgo": "tsgo --noEmit",
    "lint": "bun run --cwd ../.. eslint packages/ui --max-warnings 0 --cache --cache-location packages/ui/.eslintcache"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@tailwindcss/cli": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "@tanstack/react-form": "^1.11.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.1.5"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@tanstack/react-form": "^1.25.0",
    "@turbo/gen": "^2.6.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "bun-types": "^1.3.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/*/index.tsx"
    ]
  },
  "exports": {
    "./*": [
      "./src/components/*.tsx"
    ],
    "./hooks/*": [
      "./src/hooks/*.ts"
    ],
    "./shadcn.css": "./src/styles/shadcn.css",
    "./styles.css": "./src/styles/styles.css",
    ".": "./src/index.ts"
  }
}
