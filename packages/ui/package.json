{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "gen:component": "turbo gen react-component",
    "dev": "tailwindcss --watch -i ./src/styles/styles.css -o ./dist/styles.css",
    "build": "tailwindcss -i ./src/styles/styles.css -o ./dist/styles.css",
    "typecheck": "tsc --noEmit",
    "lint": "bun run --cwd ../.. eslint packages/ui --max-warnings 0 --cache --cache-location packages/ui/.eslintcache"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/cli": "^4.0.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.483.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.14",
    "tw-animate-css": "^1.2.4"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2"
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "@turbo/gen": "^2.3.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "bun-types": "latest",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/*/index.tsx"
    ]
  },
  "exports": {
    "./*": "./src/components/*.tsx",
    "./shadcn.css": "./src/styles/shadcn.css",
    "./styles.css": "./dist/styles.css",
    ".": "./src/index.ts"
  }
}
