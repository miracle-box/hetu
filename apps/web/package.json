{
  "name": "@repo/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --env-file .env.development.local next dev --turbopack",
    "build": "next build",
    "start": "bun --env-file .env.development.local next start",
    "typecheck": "tsc --noEmit",
    "typecheck:tsgo": "tsgo --noEmit",
    "lint": "bun run --cwd ../.. eslint apps/web --max-warnings 0 --cache --cache-location apps/web/.eslintcache"
  },
  "imports": {
    "#/*": [
      "./*.ts",
      "./*/index.ts"
    ]
  },
  "dependencies": {
    "@repo/api-client": "workspace:*",
    "@repo/ui": "workspace:*",
    "@sinclair/typebox": "^0.34.41",
    "@sinclair/typemap": "^0.10.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-form-nextjs": "^1.25.0",
    "@tanstack/react-query": "^5.90.10",
    "jose": "^6.1.2",
    "next": "^16.0.3",
    "next-intl": "^4.5.5",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "purify-ts": "^2.1.2",
    "query-string": "^9.3.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@tanstack/react-query-devtools": "^5.91.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "bun-types": "^1.3.3",
    "elysia": "^1.4.16"
  }
}
